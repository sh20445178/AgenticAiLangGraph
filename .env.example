# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Required: Google Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# AWS Configuration (Optional - uses default credential chain if not provided)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Azure Configuration (Optional - uses default credential chain if not provided)
AZURE_SUBSCRIPTION_ID=your_azure_subscription_id
AZURE_TENANT_ID=your_azure_tenant_id
AZURE_CLIENT_ID=your_azure_client_id
AZURE_CLIENT_SECRET=your_azure_client_secret
AZURE_LOCATION=eastus

# Security (Optional - for production use)
JWT_SECRET=your_jwt_secret_here

# Database (Optional - uses SQLite by default)
DATABASE_URL=postgresql://user:password@localhost:5432/agent_db

# Cache (Optional - uses in-memory cache by default)
REDIS_URL=redis://localhost:6379/0

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Logging
LOG_LEVEL=INFO
LOG_FILE=agent.log

# Development
DEBUG=false
DEVELOPMENT=false